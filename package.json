    "lodash.merge": "^4.6.0",
    "lodash.pick": "^4.2.1",
    "marky-markdown-lite": "^1.2.0",
    "minimist": "^1.2.0",
    "ora": "^3.0.0",
    "path-exists": "^3.0.0",
    "pify": "^4.0.1",
    "revalidator": "^0.3.1",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "check-for-leaks": "^1.2.0",
    "dirty-chai": "^2.0.1",
    "husky": "^1.1.2",
    "mkdirp": "^0.5.1",
